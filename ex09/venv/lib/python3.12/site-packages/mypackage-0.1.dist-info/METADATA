source ~/.zshrc
python -m venv venv
source venv/bin/activate
pip install --upgrade pip
pip install --upgrade setuptools wheel
pip install .
pip list
pip show -v ft_package
echo alias python='./venv/bin/python' >> venv/bin/activate

deactivate
source venv/bin/activate

deactivate
